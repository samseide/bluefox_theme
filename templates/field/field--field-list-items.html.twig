{% for item in items %}
  <div class="list-item">
    <div class="list-item--insides">
      <div class="list-items--title">
        <h3>{{ item.content['#paragraph'].field_title.value  }}</h3>
      </div>
      <div class="list-items--body">
        {{ item.content['#paragraph'].field_body.value|raw }}
      </div>
    </div>
  </div>
{% endfor %}
